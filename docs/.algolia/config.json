{
  "index_name": "scraper-nightly",
  "start_urls": ["https://connect.nightly.app/docs/"],
  "selectors": {
    "lvl0": ".sidebar h3.version",
    "lvl1": {
      "selector": " span",
      "global": true
    },
    "lvl2": {
      "selector": " h1",
      "strip_chars": "#"
    },
    "lvl3": {
      "selector": " h2",
      "strip_chars": "#"
    },
    "lvl4": {
      "selector": " h3",
      "strip_chars": "#"
    },
    "lvl5": {
      "selector": "h4",
      "strip_chars": "#"
    },
    "text": " p,  li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}
